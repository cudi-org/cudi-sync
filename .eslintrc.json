{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "off"
    },
    "globals": {
        "Cudi": "writable",
        "QRious": "readonly",
        "io": "readonly"
    }
}